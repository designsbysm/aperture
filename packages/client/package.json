{
  "name": "@smaperture/client",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "clean:cache": "rm -fr .expo/web/cache && rm -fr $TMPDIR/metro-cache && watchman watch-del-all",
    "preview:android": "eas build -p android --profile preview",
    "preview:ios": "eas build -p ios --profile preview",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios",
    "build:web": "expo build:web",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/pro-light-svg-icons": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/pro-solid-svg-icons": "^6.4.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@smaperture/error": "https://github.com/smaperture/ui-error#v0.1.6",
    "@smaperture/fa-icon": "https://github.com/smaperture/ui-faicon#v0.1.2",
    "@smaperture/layout": "https://github.com/smaperture/ui-layout#v0.1.4",
    "@smaperture/loading": "https://github.com/smaperture/ui-loading#v0.1.4",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "expo": "^48.0.0",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-animation-library": "^0.0.8",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-paper": "^4.12.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "~0.18.11",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.1",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.72.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageProvider": "v8",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  }
